{
  "manifest_version": 2,
  "name": "Close Meeting Tabs",
  "version": "1.0",

  "description": "closes Zoom and Webex meeting tabs for you",

  "permissions": [
    "tabs",
	"alarms",
	"storage"
  ],

  "background":{
	  "scripts": ["background-script.js"]
  },

  "options_ui":{
	"page": "settings/options.html",
	"browser_style": true,
	"chrome_style": true
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{928205e2-f473-448e-92ec-359e10ba90a1}"
    }
  }
}
